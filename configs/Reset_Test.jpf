target = SUT.SmallTest
classpath = out
native_classpath = out

vm.args = -ea
listener = Listeners.Listener_For_Uniform_Search
+uniformSearch.Thread_names = t1 t2
+uniformSearch.Thread_operations = 4 4

# JPF search settings
search.class = SearchAlgorithms.Reset_Search
+search_With_Reset.k = 5
search.multiple_errors = false

# Reporting settings
jpf.report.console.property_violation = error
report.console.finished = result,statistics,error
report.unique_errors = true
